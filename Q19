//#Q19
// Define an array of guests
const dinnerGuests: string[] = ["LOGAN", "JACKIE CHAN", "BRUCE LEE", "IP-MAN", "WICK"];

// Calculate the number of people invited
const numberOfGuests = dinnerGuests.length;

// Print a message indicating the number of people invited
console.log(`You are inviting ${numberOfGuests} people to dinner.`);
